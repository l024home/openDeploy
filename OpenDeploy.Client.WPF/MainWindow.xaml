<Window
    x:Class="OpenDeploy.Client.WPF.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls="clr-namespace:OpenDeploy.Client.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:hc="https://handyorg.github.io/handycontrol"
    xmlns:local="clr-namespace:OpenDeploy.Client.WPF"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="OpenDeploy"
    Width="800"
    Height="450"
    Background="WhiteSmoke"
    Icon="Resources\Images\favicon.ico"
    ResizeMode="CanResizeWithGrip"
    WindowStartupLocation="CenterScreen"
    mc:Ignorable="d">
    <Grid>

        <Border Padding="15">
            <WrapPanel>
                <Button x:Name="BtnTestLoading" Click="BtnTestLoading_Click">BtnTestLoading</Button>
            </WrapPanel>
        </Border>





    </Grid>
</Window>
